---
import Layout from "../../layouts/Layout.astro";

export function getStaticPaths() {
  return events;
}
export const events = [{ params: { event: "jaggo" } }, { params: { event: "haldi" } }, { params: { event: "mehndi" } }, { params: { event: "wedding" } }, { params: { event: "reception" } }];

const { event } = Astro.params;
---

<Layout dataWfPage="661327ce2bf1d37a4f852c92" dataWfSite="660d68a95235b3be5341f854" title={event}>
  <!-- <script is:inline src="https://product-gallery.cloudinary.com/all.js" type="text/javascript"></script> -->

  <article class="hero-without-image-2">
    <div class="container-9">
      <div class="hero-wrapper-two-2">
        <h1 class="heading-7">Our Work</h1>
        <p class="margin-bottom-24px-4">Crafting Timeline Memories</p>
        <div class="collection-list-wrapper-2 w-dyn-list">
          <div role="list" class="collection-list-2 w-dyn-items w-row">
            <div role="listitem" class="w-dyn-item w-col w-col-6">
              <a href="/gallery/jaggo" class="outlinebutton w-button">Jaggo</a>
            </div>
          </div>
        </div>
      </div>
    </div>
  </article>
  <section class="section-2">
    <div class="container">
      <h1 class="heading-8 w-dyn-bind-empty"></h1>
      <div class="collection-list-wrapper w-dyn-list">
        <div role="list" class="collection-list w-dyn-items">
          <div role="listitem" class="collection-item w-dyn-item">
            <a href="#" class="lightbox-link w-inline-block w-dyn-bind-empty w-lightbox">
              <img src="/images/336580366_137440855938645_6966095466362160425_n.jpg" loading="lazy" style="opacity:0" data-w-id="d390a1af-4d43-8cbe-7648-7ba3c91fcc28" alt="" class="collection-item-image w-dyn-bind-empty" />
              <script type="application/json" class="w-json">
                {
                  "items": [{
                    "url" : "/images/336580366_137440855938645_6966095466362160425_n.jpg",
                    "type" : "image"
                  }],
                  "group": ""
                }
              </script>
            </a>
          </div>
        </div>
      </div>
    </div>
  </section>
</Layout>

